{
  "name": "stylized",
  "version": "0.0.8",
  "description": "Minimalistic react component library",
  "main": "dist/index.js",
  "scripts": {
    "prepublishOnly": "babel src -d dist",
    "start": "webpack-dev-server",
    "build": "babel src -d dist && NODE_ENV=production webpack -p",
    "precommit": "lint-staged",
    "format": "prettier --single-quote --no-semi --trailing-comma es5 --write \"src/**/*.js\""
  },
  "keywords": [
    "react",
    "styled-components",
    "components",
    "flexbox"
  ],
  "author": "Tyler Knipfer",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-preset-env": "^1.5.2",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "husky": "^0.13.4",
    "lint-staged": "^3.6.1",
    "prettier": "^1.4.4",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "webpack": "^2.6.1",
    "webpack-dev-server": "^2.4.5"
  },
  "dependencies": {
    "styled-components": "^2.1.0"
  },
  "lint-staged": {
    "src/**/*.js": [
      "prettier --single-quote --no-semi --trailing-comma es5 --write",
      "git add"
    ]
  }
}
