{
  "name": "stylized",
  "version": "0.1.9",
  "description": "Stylish and fun react component library",
  "main": "dist/index.js",
  "author": "Tyler Knipfer",
  "license": "MIT",
  "repository": {
    "url": "https://github.com/knipferrc/stylized"
  },
  "bugs": {
    "url": "https://github.com/knipferrc/stylized/issues"
  },
  "keywords": [
    "react",
    "styled-components",
    "components",
    "library"
  ],
  "scripts": {
    "prepublishOnly": "rimraf dist && babel src -d dist",
    "start": "webpack-dev-server",
    "build": "rimraf dist && babel src -d dist && NODE_ENV=production webpack -p",
    "precommit": "lint-staged",
    "test": "jest",
    "format": "prettier --single-quote --no-semi --trailing-comma es5 --write \"src/**/*.js\""
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-loader": "^7.1.0",
    "babel-preset-env": "^1.5.2",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "husky": "^0.14.1",
    "jest": "^20.0.4",
    "lint-staged": "^4.0.0",
    "prettier": "^1.4.4",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-test-renderer": "^15.6.1",
    "rimraf": "^2.6.1",
    "webpack": "^3.0.0",
    "webpack-dev-server": "^2.5.0"
  },
  "dependencies": {
    "styled-components": "^2.1.0"
  },
  "lint-staged": {
    "src/**/*.js": [
      "prettier --single-quote --no-semi --trailing-comma es5 --write",
      "git add"
    ]
  }
}
